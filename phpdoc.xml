<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Corojoska</title>
    
    <parser>
        <target>doc/phpdoc</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    
    <transformer>
        <target>doc/phpdoc</target>
    </transformer>
    
    <logging>
        <level>notice</level>
    </logging>
    
    <transformations>
        <template name="clean" />
    </transformations>
    
    <files>
        <directory>src</directory>
        <ignore>src/View/*</ignore>
    </files>
</phpdoc>
